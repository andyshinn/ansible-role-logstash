# << Ansible generated file >>

input {
{% if logstash_cfg_input_redis %}
  redis {
    host      => "{{logstash_cfg_input_redis.host|default('localhost')}}"
    data_type => "{{logstash_cfg_input_redis.data_type|default('list')}}"
    key       => "{{logstash_cfg_input_redis.key|default('logstash')}}"
  }
{% endif -%}

# Local file sources
{% if logstash_cfg_input_file is defined %}
  {% for f in logstash_cfg_input_file %}
  file {
    {% for k,v in f.iteritems() %}
    {{k}} => "{{v}}"
    {% endfor -%}
  }
{% endfor -%}
{% endif -%}
}

output {
{% if logstash_cfg_output_elasticsearch is defined %}
  elasticsearch {
    {% for k,v in logstash_cfg_output_elasticsearch.iteritems() %}
    {{k}} => "{{v}}"
    {% endfor -%}
  }
{% endif -%}
}

