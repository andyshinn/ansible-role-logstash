# << Ansible generated file >>

input {

# Local file sources
{% for f in logstash_cfg_input_file %}
  file {
    {% for k,v in f.iteritems() %}
    {{k}} => {{v}}
    {% endfor -%}
  }
{% endfor %}
}

output {

{% if logstash_cfg_output_redis is defined -%}
  redis {
    host      => "{{logstash_cfg_output_redis.host}}"
    data_type => "{{logstash_cfg_output_redis.data_type}}"
    key       => "logstash"
  }
{% endif -%}

}

